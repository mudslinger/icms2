<h1>Listing libraries</h1>

<table>
  <tr>
    <th>Created by</th>
    <th>Updated by</th>
    <th>Entry</th>
    <th>Libtype</th>
    <th>Label</th>
    <th>Description</th>
    <th>File name</th>
    <th>File name ext</th>
    <th>File path</th>
    <th>Content type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @libraries.each do |library| %>
  <tr>
    <td><%= library.created_by_name %></td>
    <td><%= library.updated_by_name %></td>
    <td><%= library.entry_id %></td>
    <td><%= library.libtype %></td>
    <td><%= library.label %></td>
    <td><%= library.description %></td>
    <td><%= library.file_name %></td>
    <td><%= library.file_name_ext %></td>
    <td><%= library.file_path %></td>
    <td><%= library.content_type %></td>
    <td><%= link_to 'Show', library %></td>
    <td><%= link_to 'Edit', edit_library_path(library) %></td>
    <td><%= link_to 'Destroy', library, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= paginate @libraries %>
<%= link_to 'New Library', new_library_path %>
